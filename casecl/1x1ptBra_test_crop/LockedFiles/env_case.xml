      <schema version="3.0">$CIMEROOT/config/xml_schemas/entry_id_version3.xsd</schema>
    </entry>
    <entry id="CONFIG_ROF_FILE" value="$COMP_ROOT_DIR_ROF/cime_config/config_component.xml">

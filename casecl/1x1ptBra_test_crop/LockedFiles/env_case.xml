      <schema version="3.0">$CIMEROOT/config/xml_schemas/entry_id_version3.xsd</schema>
    </entry>
    <entry id="CONFIG_CPL_FILE_MODEL_SPECIFIC" value="$CIMEROOT/src/drivers/mct/cime_config/config_component_$MODEL.xml">

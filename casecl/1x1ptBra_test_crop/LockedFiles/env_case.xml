    </entry>
    <entry id="MACHDIR" value="/glade/u/home/lfjiang/ctsm/cime/config/cesm/machines">
      <type>char</type>
      <desc>Machines directory location</desc>
    </entry>
  </group>
  <group id="case_desc">
    <entry id="USER" value="$ENV{USER}">
